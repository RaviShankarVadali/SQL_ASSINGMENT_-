8.	SYSTEM DATE
SYSDATE
21-JAN-24
>>>select sysdate
from dual;


9.	AVERAGE SALARY PAID TO ALL EMPLOYEES
>>select avg(sal) as average_salary
from emp;
AVERAGE_SALARY
2073.214285714285714285714285714285714286





10.	HOW MANY EMPLOYEES IN EACH DEPARTMENT
>>>select deptno, count(empno)
from emp
group by deptno
order by 1;

DEPTNO	COUNT(EMPNO)
10	3
20	5
30	6

11.	TOTAL SALARY OF THE EMPLOYEES IN EACH DEPARTMENT
>>>select deptno, sum(sal)
from emp
group by deptno
DEPTNO	SUM(SAL)
10	8750
20	10875
30	9400
order by 1;

12.	HOW MANY EMPLOYEES ARE THERE UNDER EACH JOB TITLE
>>>select deptno, count(empno)
from emp
group by deptno
order by 1;

DEPTNO	COUNT(EMPNO)
10	3
20	5
30	6


13.	AVERAGE SALARY PAID FOR EACH JOB TITLE

>>>select deptno, (avg(sal))
from emp
group by deptno
order by 1;

DEPTNO	(AVG(SAL))
10	2916.666666666666666666666666666666666667
20	2175
30	1566.666666666666666666666666666666666667


