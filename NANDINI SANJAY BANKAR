26.	EMP WHO GET SALARY GREATER THAN OR EQUAL TO AVG SAL OF EMP
>>> select deptno,avg(sal), sum(sal)
from emp
group by deptno, job
    having sum(sal)>=avg(sal);

DEPTNO	AVG(SAL)	SUM(SAL)
20	2975	2975
10	5000	5000
20	3000	6000
10	1300	1300
30	1400	5600
10	2450	2450
20	950	1900
30	2850	2850
30	950	950


27.	EMP SAL<=AVG SAL
>>> select deptno,avg(sal), sum(sal)
from emp
group by deptno, job
    having sum(sal)<=avg(sal);

EPTNO	AVG(SAL)	SUM(SAL)
20	2975	2975
10	5000	5000
10	1300	1300
10	2450	2450
30	2850	2850
30	950	950


28.	EMP SAL BETWEEN 2000 AND 4000
>>> select empno, ename, job, sal, deptno
from emp
where sal between 2000 and 4000;

EMPNO	ENAME	JOB	SAL	DEPTNO
7782	CLARK	MANAGER	2450	10
7698	BLAKE	MANAGER	2850	30
7566	JONES	MANAGER	2975	20
7788	SCOTT	ANALYST	3000	20
7902	FORD	ANALYST	3000	20

29.	WHICH EMP GET HIGHEST AND LOWEST SAL
>>> select empno, ename, job, sal, deptno
from emp
    
where sal between min(sal) and max(sal) ;


30.	GIFT BUY FOR NEXT MONTH
>>> select empno, deptno, to_char (hiredate,'MM'), count(to_char(hiredate,'MM'))
    from emp
    group by empno, deptno, hiredate
    order by hiredate ;


EMPNO	DEPTNO	TO_CHAR(HIREDATE,'MM')	COUNT(TO_CHAR(HIREDATE,'MM'))
7369	20	12	1
7499	30	02	1
7521	30	02	1
